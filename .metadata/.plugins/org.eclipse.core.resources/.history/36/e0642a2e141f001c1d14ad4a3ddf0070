package modulo9.modelo;

import java.sql.SQLException;
import java.util.List;


public interface DAO {
	//responde a un patron de diseño DAO
	//CRUD. ABMC
	public boolean agregar(Model pMod) throws ClassNotFoundException, SQLException;
	public boolean modificar(Model pMod)throws ClassNotFoundException, SQLException;
	public boolean eliminar(Model pMod)throws ClassNotFoundException, SQLException;
	public List<Model> leer(Model pMod)throws ClassNotFoundException, SQLException;
	

}
