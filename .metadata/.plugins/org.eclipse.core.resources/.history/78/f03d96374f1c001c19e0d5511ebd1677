package patrones.factory;

public class Papel extends PiedraPapelTijeraFactory {
	
	public Papel() {
		setNumero(1);
		setNombre("Papel");
	}
	
	@Override
	public boolean isMe(int pNumero) {
		// TODO Auto-generated method stub
		return pNumero == 1;
	}

	@Override
	public int comparar(PiedraPapelTijeraFactory pptf) {
		// TODO Auto-generated method stub
		return 0;
	}

}
